    <script type="text/javascript" src="/all.js"></script>
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <meta property="og:image" content="http://www.uzemniplanbrno.cz/sharingpreview.png" />
    <meta name="twitter:image" content="http://www.uzemniplanbrno.cz/sharingpreview.png" />
    <title>Územní plán Brno | <%=metadata.title%></title>
    <meta property="og:title" content="Územní plán pro Brno | <%=metadata.title%>" />
